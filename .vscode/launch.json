    "version": "0.2.0",
    "configurations": [
        {
            "name": "q2 test",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/python/gh/report.py",
            "console": "integratedTerminal",
            "args": "--stdin < ~/reports/q2.txt"
        },
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}"
        },
        {
            "name": "py: with args",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "https://github.com/orgs/github/projects/8681/views/29",
                "--field",
                "Org Team",
                "\"Developer experiences\",\"Enterprise experiences\",\"Security experiences\",\"CSX - Team TBD\"",
                "--field",
                "Quarter",
                "\"FY26-Q1 (CY25-Q3)\",\"FY26-Q2 (CY25-Q4)\""
            ]
        },
        {
            "name": "test pipes.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "${fileDirname}/base64_helper.py"
            ]
        },
        // run this file golang file with the debugger
        {
            "name": "cli-module-debug",
            "type": "debugpy",
            "request": "launch",
            "module": "cli.main",
            "args": [
                "test_name"
            ]
        },
        {
            "name": "Debug migrate-branch-protections.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/python/migrate-branch-protections.py",
            "console": "integratedTerminal",
            "env": {
                "GITHUB_TOKEN": "${env:GITHUB_TOKEN}"
            }
        }
    ]
}